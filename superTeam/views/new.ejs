<%- include('./partials/header.ejs') %> 
    <form action="/cohorts/createCohort" method="POST">
        <div class="form-group">
          <label for="img_url">Logo Url</label>
          <input type="url" name="img_url" class="form-control" placeholder="e.g. https://www.example.com/some.png">
        </div>
        <div class="form-group">
          <label for="name">Team Name</label>
          <input  type="text" name="name" class="form-control" placeholder="I Don't Know ... ETC">
        </div>
        <div class="form-group">
          <label for="member">Members</label>
          <textarea type="text" name="member" rows="4" class="form-control" placeholder="Niki, Binondo, Merry, Chealsea ... ETC"></textarea>
        </div>
        <input class=" newCohort btn btn-outline-primary" type="Submit" value="submit">
      </form>
</body>
</html>